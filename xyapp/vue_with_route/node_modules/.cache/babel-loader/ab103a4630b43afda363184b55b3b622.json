'] = document.cookie.match(regex) === null ? null : document.cookie.match(regex)[1];\n  return config\n});\n\nexport default {\n  name: \"PP1\",\n  components: { NaviBox, AbstractDis1, },\n  data() {\n    return {\n      bloglist: [],\n      search_content:'',\n    }\n  },\n  mounted() {\n    this.search_content = this.$route.params.search_content,\n    this.Search()\n  },\n  methods:{\n    Search() {\n      axios.post('http://127.0.0.1:8000/json/home/', { search: this.search_content, post_type: \"Student Request\" })\n        .then((response) => {\n          console.log(response.data)\n          if (response.data['status'] != 1) {\n            alert('Sorry but you are not login')\n            this.$router.push('/');\n          } else {\n            this.bloglist = response.data['content']\n          }\n        })\n    },\n  }\n  \n}\n</script>\n\n<style scoped>\nh3 {\n  margin: 450px 0px 60px;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\na {\n  color: #000000;\n  margin: 32px;\n}\n\nb {\n  color: #000000;\n  margin: 8px;\n}\n</style>\n\n\n"]},"metadata":{},"sourceType":"module"}