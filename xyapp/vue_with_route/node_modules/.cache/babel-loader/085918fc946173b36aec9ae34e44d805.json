{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport '@element-plus/icons-vue'; // ensure csrf-token append to post requests\n// https://www.freesion.com/article/8944598323/\n\naxios.interceptors.request.use(config => {\n  config.headers['X-Requested-With'] = 'XMLHttpRequest';\n  let regex = /.*csrftoken=([^;.]*).*$/; // 用于从cookie中匹配 csrftoken值\n\n  config.headers['X-CSRFToken'] = document.cookie.match(regex) === null ? null : document.cookie.match(regex)[1];\n  return config;\n});\nimport NaviBox from '@/components/NaviBox.vue';\nexport default {\n  components: {\n    NaviBox\n  },\n  name: 'PostDetail1',\n\n  data() {\n    return {\n      title: '',\n      content: '',\n      formdata: new FormData()\n    };\n  },\n\n  methods: {\n    Post() {\n      if (this.title == \"\" || this.content == \"\") {\n        alert(\"Please check your input\");\n      } else {\n        console.log(this.formdata);\n        this.formdata.append(\"title\", this.title);\n        this.formdata.append(\"content\", this.content);\n        this.formdata.append(\"post_type\", \"Student Request\");\n        axios.post('http://127.0.0.1:8000/json/post/', this.formdata, {\n          pic: this.formdata,\n          title: this.title,\n          content: this.content,\n          post_type: \"Student Request\"\n        }).then(response => {\n          console.log(response.data);\n\n          if (response.data['status'] != 1) {\n            alert(response.data['message']);\n            this.$router.push('/');\n          } else {\n            alert(response.data['message']);\n            this.$router.push('/FirstPage');\n          }\n        });\n      }\n    },\n\n    handleelchange(file, fileList) {\n      console.log(\"123\");\n      console.log(file, fileList); //this.formdata = file.raw\n      //console.log(this.formdata)\n\n      this.formdata = new FormData();\n      fileList.map(item => {\n        //fileList本来就是数组，就不用转为真数组了\n        this.formdata.append(\"pic\", item.raw); //将每一个文件图片都加进formdata\n      });\n      console.log(this.formdata); // axios.post(\"接口地址\", formdata).then(res => { console.log(res) })\n    }\n\n  }\n  /*\n  mounted() {\n      this.showBlogs()\n      // 监听页面滚动事件\n      window.addEventListener(\"scroll\", this.showSearch)\n  },\n  */\n\n};","map":{"version":3,"mappings":";AA4CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAgB,yBAAhB,C,CAEA;AACA;;AACAA,KAAK,CAACC,YAAN,CAAmBC,OAAnB,CAA2BC,GAA3B,CAAgCC,MAAD,IAAY;EACzCA,MAAM,CAACC,OAAP,CAAe,kBAAf,IAAqC,gBAArC;EACA,IAAIC,KAAI,GAAI,yBAAZ,CAFyC,CAEF;;EACvCF,MAAM,CAACC,OAAP,CAAe,aAAf,IAAgCE,QAAQ,CAACC,MAAT,CAAgBC,KAAhB,CAAsBH,KAAtB,MAAiC,IAAjC,GAAwC,IAAxC,GAA+CC,QAAQ,CAACC,MAAT,CAAgBC,KAAhB,CAAsBH,KAAtB,EAA6B,CAA7B,CAA/E;EACA,OAAOF,MAAP;AACD,CALD;AAQA,OAAOM,OAAP,MAAoB,0BAApB;AACA,eAAe;EACbC,UAAU,EAAE;IAAED;EAAF,CADC;EAEbE,IAAI,EAAE,aAFO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,OAAO,EAAE,EAFJ;MAGLC,QAAQ,EAAE,IAAIC,QAAJ;IAHL,CAAP;EAKD,CATY;;EAUbC,OAAO,EAAE;IACPC,IAAI,GAAG;MACL,IAAI,KAAKL,KAAL,IAAc,EAAd,IAAoB,KAAKC,OAAL,IAAgB,EAAxC,EAA4C;QAC1CK,KAAK,CAAC,yBAAD,CAAL;MACF,CAFA,MAEO;QACLC,OAAO,CAACC,GAAR,CAAY,KAAKN,QAAjB;QACA,KAAKA,QAAL,CAAcO,MAAd,CAAqB,OAArB,EAA8B,KAAKT,KAAnC;QACA,KAAKE,QAAL,CAAcO,MAAd,CAAqB,SAArB,EAAgC,KAAKR,OAArC;QACA,KAAKC,QAAL,CAAcO,MAAd,CAAqB,WAArB,EAAkC,iBAAlC;QACAvB,KAAK,CAACwB,IAAN,CAAW,kCAAX,EAA8C,KAAKR,QAAnD,EAA4D;UAACS,GAAG,EAAC,KAAKT,QAAV;UAAqBF,KAAK,EAAE,KAAKA,KAAjC;UAAwCC,OAAO,EAAE,KAAKA,OAAtD;UAA+DW,SAAS,EAAE;QAA1E,CAA5D,EACGC,IADH,CACSC,QAAD,IAAc;UAClBP,OAAO,CAACC,GAAR,CAAYM,QAAQ,CAACf,IAArB;;UACA,IAAIe,QAAQ,CAACf,IAAT,CAAc,QAAd,KAA2B,CAA/B,EAAkC;YAChCO,KAAK,CAACQ,QAAQ,CAACf,IAAT,CAAc,SAAd,CAAD,CAAL;YACA,KAAKgB,OAAL,CAAaC,IAAb,CAAkB,GAAlB;UACF,CAHA,MAGO;YACLV,KAAK,CAACQ,QAAQ,CAACf,IAAT,CAAc,SAAd,CAAD,CAAL;YACA,KAAKgB,OAAL,CAAaC,IAAb,CAAkB,YAAlB;UACF;QACD,CAVH;MAWF;IACD,CArBM;;IAsBPC,cAAc,CAACC,IAAD,EAAOC,QAAP,EAAiB;MAC7BZ,OAAO,CAACC,GAAR,CAAY,KAAZ;MACAD,OAAO,CAACC,GAAR,CAAYU,IAAZ,EAAkBC,QAAlB,EAF6B,CAG7B;MACA;;MAEA,KAAKjB,QAAL,GAAgB,IAAIC,QAAJ,EAAhB;MACAgB,QAAQ,CAACC,GAAT,CAAaC,IAAG,IAAK;QAAE;QACrB,KAAKnB,QAAL,CAAcO,MAAd,CAAqB,KAArB,EAA4BY,IAAI,CAACC,GAAjC,EADmB,CACoB;MACxC,CAFD;MAGAf,OAAO,CAACC,GAAR,CAAY,KAAKN,QAAjB,EAV6B,CAW7B;IACD;;EAlCM;EAoCT;;;;;;;;AA9Ca,CAAf","names":["axios","interceptors","request","use","config","headers","regex","document","cookie","match","NaviBox","components","name","data","title","content","formdata","FormData","methods","Post","alert","console","log","append","post","pic","post_type","then","response","$router","push","handleelchange","file","fileList","map","item","raw"],"sourceRoot":"","sources":["/Users/hongjunwu/Desktop/Hw/2022_SE/xyapp/vue_with_route/src/views/PostDetail1.vue"],"sourcesContent":["<!-- 说明：同学互助信息 发布页\n内容：\n根据板块不同有三个入口——每个入口一个vue UI，这里只实现了一种\n个人信息，内容简介，PS须知或其他事项，图片描述——图片描述 -->\n\n<template>\n  <div class=\"home\">\n    <h1>\n      <img src=\"@/assets/logo.svg\">\n    </h1>\n    <h1>Student Request Post</h1>\n    <h2>\n      <p>\n        <t style=\"margin-right: 200px\">Name</t><br>\n        <textarea maxlength=\"25\" placeholder=\"less than 25 characters\" cols=\"32\" style=\"resize:none;\"\n          v-model=\"title\"></textarea><br>\n      </p>\n      <p>\n        <t style=\"margin-right: 185px\">Abstract</t><br>\n        <textarea maxlength=\"25\" placeholder=\"less than 25 characters\" cols=\"32\" style=\"resize:none;\"></textarea><br>\n      </p>\n      <p>\n        <t style=\"margin-right: 205px\">Detail</t><br>\n        <textarea maxlength=\"500\" placeholder=\"less than 500 characters\" cols=\"32\" rows=\"10\" style=\"resize:none;\"\n          v-model=\"content\"></textarea><br>\n      </p>\n        <t style=\"margin-right: 205px\">Photo</t><br>\n      <el-upload action=\"\" :on-change=\"handleelchange\"  :auto-upload=\"false\" list-type=\"picture-card\" accept=\"image/png,image/gif,image/jpg,image/jpeg\" :limit=\"1\">\n        <i class=\"el-icon-plus\">Upload</i>\n      </el-upload>\n      <el-button style=\"margin-left: 14%; \" @click=\"Post()\" type=\"text\">\n        <img src=\"@/assets/send.svg\">\n      </el-button>\n    </h2>\n\n\n    <!-- TODO：增加图片上传按钮和功能 -->\n\n    <navi-box></navi-box>\n\n  </div>\n</template>\n  \n<script>\nimport axios from \"axios\"\nimport { } from '@element-plus/icons-vue'\n\n// ensure csrf-token append to post requests\n// https://www.freesion.com/article/8944598323/\naxios.interceptors.request.use((config) => {\n  config.headers['X-Requested-With'] = 'XMLHttpRequest';\n  let regex = /.*csrftoken=([^;.]*).*$/; // 用于从cookie中匹配 csrftoken值\n  config.headers['X-CSRFToken'] = document.cookie.match(regex) === null ? null : document.cookie.match(regex)[1];\n  return config\n});\n\n\nimport NaviBox from '@/components/NaviBox.vue'\nexport default {\n  components: { NaviBox },\n  name: 'PostDetail1',\n  data() {\n    return {\n      title: '',\n      content: '',\n      formdata: new FormData(),\n    }\n  },\n  methods: {\n    Post() {\n      if (this.title == \"\" || this.content == \"\") {\n        alert(\"Please check your input\")\n      } else {\n        console.log(this.formdata)\n        this.formdata.append(\"title\", this.title)\n        this.formdata.append(\"content\", this.content)\n        this.formdata.append(\"post_type\", \"Student Request\")\n        axios.post('http://127.0.0.1:8000/json/post/',this.formdata,{pic:this.formdata,  title: this.title, content: this.content, post_type: \"Student Request\" })\n          .then((response) => {\n            console.log(response.data)\n            if (response.data['status'] != 1) {\n              alert(response.data['message'])\n              this.$router.push('/');\n            } else {\n              alert(response.data['message'])\n              this.$router.push('/FirstPage');\n            }\n          })\n      }\n    },\n    handleelchange(file, fileList) {\n      console.log(\"123\")\n      console.log(file, fileList)\n      //this.formdata = file.raw\n      //console.log(this.formdata)\n      \n      this.formdata = new FormData()\n      fileList.map(item => { //fileList本来就是数组，就不用转为真数组了\n        this.formdata.append(\"pic\", item.raw)  //将每一个文件图片都加进formdata\n      })\n      console.log(this.formdata)\n      // axios.post(\"接口地址\", formdata).then(res => { console.log(res) })\n    },\n  }\n  /*\n  mounted() {\n      this.showBlogs()\n      // 监听页面滚动事件\n      window.addEventListener(\"scroll\", this.showSearch)\n  },\n  */\n}\n</script>\n  \n<style scoped>\nh2 {\n  font: 1em sans-serif;\n  margin: 0px 0px 60px;\n}\n\na {\n  color: #000000;\n  margin: 32px;\n}\n\nh3 {\n  margin: 0px 0px 60px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}