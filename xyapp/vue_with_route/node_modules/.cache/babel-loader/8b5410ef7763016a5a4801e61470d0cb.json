{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NaviBox from '@/components/NaviBox.vue';\nimport AbstractDis2 from '@/components/AbstractDis2.vue';\nexport default {\n  components: {\n    NaviBox,\n    AbstractDis2\n  },\n  name: 'PP2',\n\n  data() {\n    return {\n      bloglist: []\n    };\n  },\n\n  mounted() {\n    this.showBlogs(); // 监听页面滚动事件\n\n    window.addEventListener(\"scroll\", this.showSearch);\n  },\n\n  methods: {\n    showBlogs() {\n      axios.post('http://127.0.0.1:8000/json/showpost/').then(response => {\n        console.log(response.data);\n\n        if (response.data['status'] != 1) {\n          alert('Sorry but you are not login');\n          this.$router.push('/');\n        } else {\n          this.bloglist = response.data['content'];\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAiCA,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,YAAP,MAAyB,+BAAzB;AACA,eAAe;EACbC,UAAU,EAAE;IAAEF,OAAF;IAAWC;EAAX,CADC;EAEbE,IAAI,EAAE,KAFO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IADL,CAAP;EAGD,CAPY;;EAQbC,OAAO,GAAG;IACR,KAAKC,SAAL,GADQ,CAER;;IACAC,MAAM,CAACC,gBAAP,CAAwB,QAAxB,EAAkC,KAAKC,UAAvC;EACD,CAZY;;EAabC,OAAO,EAAE;IACPJ,SAAS,GAAG;MACVK,KAAK,CAACC,IAAN,CAAW,sCAAX,EACGC,IADH,CACSC,QAAD,IAAc;QAClBC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACX,IAArB;;QACA,IAAIW,QAAQ,CAACX,IAAT,CAAc,QAAd,KAA2B,CAA/B,EAAkC;UAChCc,KAAK,CAAC,6BAAD,CAAL;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,GAAlB;QACF,CAHA,MAGO;UACL,KAAKf,QAAL,GAAgBU,QAAQ,CAACX,IAAT,CAAc,SAAd,CAAhB;QACF;MACD,CATH;IAUF;;EAZO;AAbI,CAAf","names":["NaviBox","AbstractDis2","components","name","data","bloglist","mounted","showBlogs","window","addEventListener","showSearch","methods","axios","post","then","response","console","log","alert","$router","push"],"sourceRoot":"","sources":["C:\\Users\\郝泽成\\2022_SE-hzc_code\\myproject_v9\\myproject\\2022_SE\\2022_SE\\xyapp\\vue_with_route\\src\\views\\PP2Result.vue"],"sourcesContent":["<!-- 说明：科研板块结果页\r\n内容：\r\n注意！！没有搜索框\r\n显示热门内容——至多100条\r\n底部功能栏——滑框 -->\r\n\r\n<template>\r\n  <div class=\"PP2\">\r\n    //<abstract-dis-2></abstract-dis-2><br>\r\n    //<abstract-dis-2></abstract-dis-2><br>\r\n    //<abstract-dis-2></abstract-dis-2><br>\r\n\r\n    <AbstractDis2 v-for=\"blog in bloglist\" :key=\"blog.title\" :title=\"blog.title\" :summary=\"blog.summary\"\r\n      :date_posted=\"blog.date_posted\">\r\n    </AbstractDis2><br>\r\n\r\n\r\n    <router-view></router-view>\r\n    <navi-box></navi-box>\r\n  </div>\r\n</template>\r\n\r\nimport axios from \"axios\"\r\n// ensure csrf-token append to post requests\r\n// https://www.freesion.com/article/8944598323/\r\naxios.interceptors.request.use((config) => {\r\n  config.headers['X-Requested-With'] = 'XMLHttpRequest';\r\n  let regex = /.*csrftoken=([^;.]*).*$/; // 用于从cookie中匹配 csrftoken值\r\n  config.headers['X-CSRFToken'] = document.cookie.match(regex) === null ? null : document.cookie.match(regex)[1];\r\n  return config\r\n});\r\n\r\n<script>\r\nimport NaviBox from '@/components/NaviBox.vue'\r\nimport AbstractDis2 from '@/components/AbstractDis2.vue'\r\nexport default {\r\n  components: { NaviBox, AbstractDis2 },\r\n  name: 'PP2',\r\n  data() {\r\n    return {\r\n      bloglist: [],\r\n    }\r\n  },\r\n  mounted() {\r\n    this.showBlogs()\r\n    // 监听页面滚动事件\r\n    window.addEventListener(\"scroll\", this.showSearch)\r\n  },\r\n  methods: {\r\n    showBlogs() {\r\n      axios.post('http://127.0.0.1:8000/json/showpost/')\r\n        .then((response) => {\r\n          console.log(response.data)\r\n          if (response.data['status'] != 1) {\r\n            alert('Sorry but you are not login')\r\n            this.$router.push('/');\r\n          } else {\r\n            this.bloglist = response.data['content']\r\n          }\r\n        })\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nh3 {\r\n  margin: 600px 0px 60px;\r\n}\r\n\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\na {\r\n  color: #000000;\r\n  margin: 32px;\r\n}\r\n\r\nb {\r\n  color: #000000;\r\n  margin: 8px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}