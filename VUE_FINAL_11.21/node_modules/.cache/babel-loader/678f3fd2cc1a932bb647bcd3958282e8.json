{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport '@element-plus/icons-vue';\nconst baseURL = 'http://localhost:3000';\naxios.defaults.baseURL = baseURL;\n\nconst loginCheck = params => {\n  return axios.post('/login', params).then(res => {\n    return res.data;\n  });\n};\n\nexport default {\n  name: \"HelloWorld\",\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n\n  methods: {\n    async Login() {\n      this.form = \"&username:\" + this.username + \"&password:\" + this.password; // 调用登录请求接口\n\n      loginCheck(this.form).then(res => {\n        // 登录成功,提示成功信息，然后跳转到首页\n        if (res.meta.status === 200) {\n          // 提示成功信息\n          this.$message({\n            message: res.meta.msg,\n            type: 'success'\n          });\n          var that = this; // 跳转到首页\n\n          setTimeout(function () {\n            that.$router.push({\n              name: 'FirstPage'\n            });\n          }, 1000);\n        } else {\n          // 登录失败，提示错误信息\n          this.$message({\n            message: 'Login Failed,' + res.meta.msg,\n            type: 'error'\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA8DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAgB,yBAAhB;AAEA,MAAMC,OAAM,GAAI,uBAAhB;AACAD,KAAK,CAACE,QAAN,CAAeD,OAAf,GAAyBA,OAAzB;;AACA,MAAME,UAAS,GAAIC,MAAK,IAAK;EAC3B,OAAOJ,KAAK,CAACK,IAAN,CAAW,QAAX,EAAqBD,MAArB,EAA6BE,IAA7B,CAAkCC,GAAE,IAAK;IAC5C,OAAOA,GAAG,CAACC,IAAX;EACH,CAFM,CAAP;AAGF,CAJA;;AAMA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,KAAK,EAAE;IACHC,GAAG,EAAEC;EADF,CAFM;;EAKbJ,IAAI,GAAG;IACL,OAAO;MACLK,QAAQ,EAAE,EADL;MAELC,QAAQ,EAAE;IAFL,CAAP;EAID,CAVY;;EAWbC,OAAO,EAAC;IACN,MAAMC,KAAN,GAAa;MACX,KAAKC,IAAL,GAAY,eAAe,KAAKJ,QAApB,GAA+B,YAA/B,GAA8C,KAAKC,QAA/D,CADW,CAEX;;MACAX,UAAU,CAAC,KAAKc,IAAN,CAAV,CAAsBX,IAAtB,CAA2BC,GAAE,IAAK;QAChC;QACA,IAAGA,GAAG,CAACW,IAAJ,CAASC,MAAT,KAAoB,GAAvB,EAA2B;UACzB;UACA,KAAKC,QAAL,CAA